{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch main.go",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}/cmd/main.go",
            "cwd": "${workspaceFolder}",
            "args": [],
            "showLog": true
        },
        {
            "name": "Current file",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${file}",
            "cwd": "${workspaceFolder}",
            "showLog": true,
            "console": "integratedTerminal"
        }
    ]
}
